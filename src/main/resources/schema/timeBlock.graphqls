extend type Query {
    timeBlocks(ids: [ID!], trackedTaskId: ID): [TimeBlock!]
}

extend type Mutation {
    createTimeBlock(timeSlot: Int!, trackedTaskId: ID!): TimeBlock!
    deleteTimeBlock(id: ID!): Boolean
}
type TimeBlock {
    id: ID!
    # a day is broken up into 6min time slots. 0 = 00:00, 1 = 00:06, 2 = 00:12 etc
    timeSlot: Int!
}
